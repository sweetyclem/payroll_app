<% flash.each do |key, value| %>
  <div class="<%= flash_class(key) %>">
    <%= value %>
  </div>
<% end %>

<h1>Payroll Reports</h1>

<br><br>
<table class="responstable">
  <tr>
    <th><%= t('en.payroll_report.id') %></th>
    <th><%= t('en.payroll_report.employee_id') %></th>
    <th><%= t('en.payroll_report.pay_period') %></th>
    <th><%= t('en.payroll_report.amount_paid') %></th>
  </tr>
  <%= render @payroll_reports %>
</table>
